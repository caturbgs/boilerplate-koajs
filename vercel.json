{
  "version": 2,
  "name": "boilerplate-koajs",
  "builds": [
    {
      "src": "./build/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "build/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/index.js"
    }
  ]
}
